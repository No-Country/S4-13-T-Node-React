            "type": "number"
          },
          {
            "in": "query",
            "name": "size",
            "description": "size of posts",
            "type": "number"
          },
          {
            "in": "query",
            "name": "sort",
            "description": "change between return older posts or newer",
            "enum": ["desc", "asc"],
            "type": "string"
